<Window x:Class="wpfAplicacionesEscritorio.Modulo1.wpf4"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:wpfAplicacionesEscritorio.Modulo1"
        mc:Ignorable="d"
        Title="wpf4" Height="450" Width="800">
    <StackPanel Width="200">
        <Button Content="Hola"></Button>
        <Button Margin="0,20,0,0">
            <StackPanel Orientation="Horizontal">
                <Rectangle Width="20" Fill="Orange">
                </Rectangle>
                <TextBlock Text=" Hola"></TextBlock>
                <TextBlock Text=" Mundo"></TextBlock>
            </StackPanel>
        </Button>
        <TextBox Text="Teste"></TextBox>
        <DatePicker></DatePicker>
        <Calendar></Calendar>
        <CheckBox></CheckBox>
        <RadioButton></RadioButton>
        <RadioButton></RadioButton>
        <ComboBox></ComboBox>
    </StackPanel>
</Window>
